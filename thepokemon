# the pokemon with attributes name, hp=health points, moves, accuracy
class Pokemon:
    def __init__(self, name, hp, moves, accuracy):
        self.name=name
        self.hp=hp
        self.moves=moves
        self.acc=accuracy   #flash will decrease accuracy
        
charizard=Pokemon("Charizard", 110, ["slash", "fire spin", "fire blast"],0)
raichu=Pokemon("Raichu", 90, ["swift", "thunderbolt", "flash"],0)

    

